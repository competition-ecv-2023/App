{
  "expo": {
    "name": "pat-perdue",
    "slug": "pat-perdue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/patperdue_splash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F19B3F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/patperdue_logo.png",
        "backgroundColor": "#FF0000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "SENTRY_ORG",
            "project": "SENTRY_PROJECT"
          }
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "f118d354-5daf-4d7b-8ccb-d91ebcbfdc60"
      }
    },
    "owner": "aubrian-ecv",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f118d354-5daf-4d7b-8ccb-d91ebcbfdc60"
    }
  }
}
